    const animateCursor = () => {
      if (cursorDotRef.current && cursorOutlineRef.current) {
        cursorDotRef.current.style.top = `${mousePosition.y}px`;
        cursorDotRef.current.style.left = `${mousePosition.x}px`;

        cursorOutlineRef.current.style.top = `${mousePosition.y}px`;
        cursorOutlineRef.current.style.left = `${mousePosition.x}px`;
      }
      requestAnimationFrame(animateCursor);
    };
    animateCursor();
  }, [mousePosition]);

  return (
    <>
      <div
        ref={cursorDotRef}
        className="cursor-dot"
        style={{
          position: "absolute",
          pointerEvents: "none",
          zIndex: 1,
          willChange: "transform",
        }}
      />
      <div
        ref={cursorOutlineRef}
        className="cursor-outline"
        style={{
          position: "absolute",
          pointerEvents: "none",
          zIndex: 1,
          willChange: "transform",
        }}
      />
    </>
  );
};

export default CursorDot;
